	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#666666",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": ["string", "punctuation.definition.string"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Built-in constant",
			"scope": ["constant.language"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Storage",
			"scope": ["storage"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Storage type",
			"scope": ["storage.type"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Class name",
			"scope": ["entity.name.class"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Inherited class",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Function name",
			"scope": ["entity.name.function"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Function argument",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Tag name",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Tag attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Library function",
			"scope": ["support.function"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Library constant",
			"scope": ["support.constant"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "Library class/type",
			"scope": ["support.type", "support.class"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "Library variable",
			"scope": ["support.other.variable"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "JSON String",
			"scope": ["meta.structure.dictionary.json string.quoted.double.json"],
			"settings": {
				"foreground": "#87CEEB"
			}
		},
		{
			"name": "diff.header",
			"scope": ["meta.diff", "meta.diff.header"],
			"settings": {
				"foreground": "#00BFFF"
			}
		},
		{
			"name": "diff.deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "diff.inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#00FF00"
			}
		},
		{
			"name": "diff.changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#FFFF00"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#87CEEB"
			}
		}
	]
}