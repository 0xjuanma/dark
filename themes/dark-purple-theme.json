{
	"name": "0xDark Purple",
	"type": "dark",
	"colors": {
		"activityBar.activeBackground": "#1a1a1a",
		"activityBar.activeBorder": "#FF00FF",
		"activityBar.background": "#000000",
		"activityBar.border": "#1a1a1a",
		"activityBar.foreground": "#cccccc",
		"activityBarBadge.background": "#00FFFF",
		"badge.foreground": "#000000",
		"button.background": "#1a1a1a",
		"button.foreground": "#ffffff",
		"debugToolBar.background": "#000000",
		"dropdown.background": "#000000",
		"editor.background": "#000000",
		"editor.findMatchBackground": "#1a1a1a",
		"editor.findMatchHighlightBackground": "#0d0d0d",
		"editor.foreground": "#cccccc",
		"editor.inactiveSelectionBackground": "#1a1a1a",
		"editor.selectionBackground": "#1a1a1a",
		"editor.selectionHighlightBackground": "#0d0d0d",
		"editorIndentGuide.activeBackground": "#404040",
		"editorIndentGuide.background": "#1a1a1a",
		"input.background": "#000000",
		"input.foreground": "#ffffff",
		"input.placeholderForeground": "#666666",
		"list.dropBackground": "#1a1a1a",
		"menu.background": "#0d0d0d",
		"menu.foreground": "#cccccc",
		"panel.background": "#000000",
		"panel.border": "#1a1a1a",
		"panelSectionHeader.foreground": "#cccccc",
		"settings.numberInputBackground": "#0d0d0d",
		"settings.textInputBackground": "#0d0d0d",
		"sideBar.background": "#000000",
		"sideBar.foreground": "#cccccc",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#cccccc33",
		"sideBarTitle.foreground": "#aaaaaa",
		"statusBar.background": "#1a1a1a",
		"statusBar.debuggingBackground": "#000000",
		"statusBar.debuggingForeground": "#1a1a1a",
		"statusBar.foreground": "#000000",
		"statusBarItem.remoteBackground": "#CC00CC",
		"statusBarItem.remoteForeground": "#ffffff",
		"terminal.foreground": "#cccccc"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#555555"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF4444"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Operator, Control, Punctuation",
			"scope": [
				"keyword.control",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#DD66DD"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#EE88EE"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#FF99FF"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter"
			],
			"settings": {
				"foreground": "#00FF99"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#00CCCC"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#66CCCC"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#00CCCC"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": ["entity.name.method.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#DD66DD"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": {
				"foreground": "#00CCCC"
			}
		},
		{
			"name": "CSS ID's",
			"scope": ["source.sass keyword.control"],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#00FF99"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF4444"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#FF99FF"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#00FF99"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#DD66DD"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "JSON Key - Level 1",
			"scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
			"settings": {
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "JSON Key - Level 2",
			"scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "JSON Key - Level 3+",
			"scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
			"settings": {
				"foreground": "#00CCCC"
			}
		},
		{
			"name": "Markdown - Plain",
			"scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline",
			"scope": ["text.html.markdown markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#66CCCC"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline Punctuation",
			"scope": ["text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#DD66DD"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold", "markup.bold string"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CC00CC"
			}
		},
		{
			"name": "Markup - Bold-Italic",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold",
				"markup.quote markup.bold",
				"markup.bold markup.italic string",
				"markup.italic markup.bold string",
				"markup.quote markup.bold string"
			],
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#FF00FF"
			}
		},
		{
			"name": "Markup - Underline",
			"scope": ["markup.underline"],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#FF99FF"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": ["markup.quote punctuation.definition.blockquote.markdown"],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Markup - Quote",
			"scope": ["markup.quote"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Link",
			"scope": ["string.other.link.title.markdown"],
			"settings": {
				"foreground": "#00FFFF"
			}
		},
		{
			"name": "Markdown - Link Description",
			"scope": ["string.other.link.description.title.markdown"],
			"settings": {
				"foreground": "#66CCCC"
			}
		},
		{
			"name": "Markdown - Link Anchor",
			"scope": ["constant.other.reference.link.markdown"],
			"settings": {
				"foreground": "#00FF99"
			}
		},
		{
			"name": "Markup - Raw Block",
			"scope": ["markup.raw.block"],
			"settings": {
				"foreground": "#66CCCC"
			}
		},
		{
			"name": "Markdown - Fenced Code Block",
			"scope": [
				"markup.raw.block.fenced.markdown",
				"punctuation.definition.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000050"
			}
		},
		{
			"name": "Markdown - Fenced Language",
			"scope": ["variable.language.fenced.markdown"],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Markdown - Separator",
			"scope": ["meta.separator"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#555555"
			}
		},
		{
			"name": "Markup - Table",
			"scope": ["markup.table"],
			"settings": {
				"foreground": "#cccccc"
			}
		}
	]
}
