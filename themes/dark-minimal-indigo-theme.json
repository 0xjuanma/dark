{
	"name": "0xDark Minimal Indigo",
	"type": "dark",
	"colors": {
		"activityBar.activeBackground": "#1a1a1a",
		"activityBar.activeBorder": "#6B5BFF",
		"activityBar.background": "#000000",
		"activityBar.border": "#1a1a1a",
		"activityBar.foreground": "#cccccc",
		"activityBarBadge.background": "#FFD166",
		"badge.foreground": "#000000",
		"button.background": "#1a1a1a",
		"button.foreground": "#ffffff",
		"debugToolBar.background": "#000000",
		"dropdown.background": "#000000",
		"editor.background": "#000000",
		"editor.findMatchBackground": "#1a1a1a",
		"editor.findMatchHighlightBackground": "#0d0d0d",
		"editor.foreground": "#cccccc",
		"editor.inactiveSelectionBackground": "#1a1a1a",
		"editor.selectionBackground": "#1a1a1a",
		"editor.selectionHighlightBackground": "#0d0d0d",
		"editorIndentGuide.activeBackground": "#404040",
		"editorIndentGuide.background": "#1a1a1a",
		"input.background": "#000000",
		"input.foreground": "#ffffff",
		"input.placeholderForeground": "#555555",
		"list.dropBackground": "#1a1a1a",
		"menu.background": "#0d0d0d",
		"menu.foreground": "#cccccc",
		"panel.background": "#000000",
		"panel.border": "#1a1a1a",
		"panelSectionHeader.foreground": "#cccccc",
		"settings.numberInputBackground": "#0d0d0d",
		"settings.textInputBackground": "#0d0d0d",
		"sideBar.background": "#000000",
		"sideBar.foreground": "#cccccc",
		"sideBarSectionHeader.background": "#00000000",
		"sideBarSectionHeader.border": "#cccccc33",
		"sideBarTitle.foreground": "#aaaaaa",
		"statusBar.background": "#1a1a1a",
		"statusBar.debuggingBackground": "#000000",
		"statusBar.debuggingForeground": "#1a1a1a",
		"statusBar.foreground": "#000000",
		"statusBarItem.remoteBackground": "#6B5BFF",
		"statusBarItem.remoteForeground": "#ffffff",
		"terminal.foreground": "#cccccc"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#555555",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": ["string", "punctuation.definition.string"],
			"settings": {
				"foreground": "#FFD166"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Built-in constant",
			"scope": ["constant.language"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword"],
			"settings": {
				"foreground": "#6B5BFF"
			}
		},
		{
			"name": "Storage",
			"scope": ["storage"],
			"settings": {
				"foreground": "#6B5BFF"
			}
		},
		{
			"name": "Storage type",
			"scope": ["storage.type"],
			"settings": {
				"foreground": "#6B5BFF"
			}
		},
		{
			"name": "Class name",
			"scope": ["entity.name.class"],
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"name": "Inherited class",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Function name",
			"scope": ["entity.name.function"],
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"name": "Function argument",
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Tag name",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#6B5BFF"
			}
		},
		{
			"name": "Tag attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Library function",
			"scope": ["support.function"],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Library constant",
			"scope": ["support.constant"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Library class/type",
			"scope": ["support.type", "support.class"],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Library variable",
			"scope": ["support.other.variable"],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid"],
			"settings": {
				"foreground": "#FF6666"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#FF6666"
			}
		},
		{
			"name": "JSON String",
			"scope": ["meta.structure.dictionary.json string.quoted.double.json"],
			"settings": {
				"foreground": "#FFD166"
			}
		},
		{
			"name": "JSON Key",
			"scope": ["support.type.property-name.json"],
			"settings": {
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "diff.header",
			"scope": ["meta.diff", "meta.diff.header"],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "diff.deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#FF6666"
			}
		},
		{
			"name": "diff.inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#88CC88"
			}
		},
		{
			"name": "diff.changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#FFD166"
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#6B5BFF"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": ["markup.bold", "markup.bold string"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Markdown - Link",
			"scope": ["string.other.link.title.markdown"],
			"settings": {
				"foreground": "#FFD166"
			}
		},
		{
			"name": "Markdown - Code",
			"scope": ["text.html.markdown markup.inline.raw.markdown"],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#bbbbbb"
			}
		}
	]
}
